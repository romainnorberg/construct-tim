{
    "name": "vagrant/construct.tim.be",
    "description": "Construct-tim composer",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Romain Norberg",
            "email": "romainnorberg@gmail.com",
            "homepage": "http://romainnorberg.be"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.0.6",
        "doctrine/orm": "^2.5",
        "symfony/form": "^3.0",
        "symfony/security": "^3.0",
        "symfony/console": "^3.0",
        "symfony/doctrine-bridge": "^3.0",
        "symfony/monolog-bundle": "^2.8",
        "symfony/framework-bundle": "^3.0",
        "symfony/security-acl": "^3.0",
        "sonata-project/exporter": "^1.3.1",
        "symfony/swiftmailer-bundle": "~2.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "staffim/rollbar-bundle": "1.3.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "excelwebzone/recaptcha-bundle": "3.0.x-dev",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "vich/uploader-bundle": "1.0.1",
        "knplabs/knp-gaufrette-bundle": "^0.3.0",
        "league/flysystem-aws-s3-v3": "1.0.11",
        "liip/imagine-bundle": "1.5.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "simplethings/entity-audit-bundle": "~0.1",
        "doctrine/doctrine-fixtures-bundle": "2.3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.0"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
